{
    "name": "user-library-demo",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test --watch=true --code-coverage=true --source-map=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "local": "ng serve --proxy-config proxy.api.conf.json --port 5200",
        "mock:server": "json-server --watch mocks/mockData.json",
        "start:proxy": "ng serve --proxy-config proxy.conf.json",
        "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\""
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.0",
