        "@angular/platform-browser": "^8.2.11",
        "@angular/platform-browser-dynamic": "^8.2.11",
        "@angular/router": "^8.2.11",
        "bootstrap": "^4.3.1",
        "jquery": "^3.4.1",
        "moment": "^2.24.0",
        "ng-bootstrap-form-validation": "^5.0.0",
        "ngx-bootstrap": "^5.2.0",
        "ngx-toastr": "^11.1.2",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.0",
